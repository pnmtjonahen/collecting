    "hammerjs": "2.0.8",
    "rxjs": "7.8.1",
    "showdown": "2.1.0",
    "swiper": "^10.2.0",
    "tslib": "2.6.2",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1602.0",
